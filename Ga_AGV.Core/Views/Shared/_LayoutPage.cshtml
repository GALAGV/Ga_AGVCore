<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <!-- bootstrap样式文件 -->
    <link href="~/Scripts/bootstrap-3.3.7/css/bootstrap.min.css" rel="stylesheet" />
    <!-- bootstrap默认主题 -->
    <link href="~/Scripts/bootstrap-3.3.7/css/bootstrap-theme.min.css" rel="stylesheet" />
    <!-- 通用样式 -->
    <link href="~/Content/commonality.css" rel="stylesheet" />
    <!--animate动画文件 -->
    <link href="~/Content/animate.min.css" rel="stylesheet" />
    <!-- table表格组件样式 -->
    <link href="~/Content/bootstrap-table/bootstrap-table.css" rel="stylesheet" />
    <!-- 日期组件样式 -->
    <link href="~/Scripts/bootstrap-datetimepicker-master/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <!-- 下拉框组件样式 -->
    <link href="~/Scripts/bootstrap-select-1.13.2/dist/css/bootstrap-select.min.css" rel="stylesheet" />
    <!--字体库图标库 -->
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <!--提示框组件样式 -->
    <link href="~/Scripts/toastr-master/toastr.min.css" rel="stylesheet" />
    <!--jquery库 -->
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <!--bootstrap库 -->
    <script src="~/Scripts/bootstrap-3.3.7/js/bootstrap.min.js"></script>
    <!--table组件 -->
    <script src="~/Content/bootstrap-table/bootstrap-table.js"></script>
    <!--日历组件 -->
    <script src="~/Scripts/bootstrap-datetimepicker-master/js/bootstrap-datetimepicker.js"></script>
    <!--下拉框组件 -->
    <script src="~/Scripts/bootstrap-select-1.13.2/dist/js/bootstrap-select.min.js"></script>
    <!--table组件语言包 -->
    <script src="~/Content/bootstrap-table/locale/bootstrap-table-zh-CN.min.js"></script>
    <!--下拉框组件语言包 -->
    <script src="~/Scripts/bootstrap-select-1.13.2/dist/js/i18n/defaults-zh_CN.min.js"></script>
    <!--日历组件语言包 -->
    <script src="~/Scripts/bootstrap-datetimepicker-master/js/locales/bootstrap-datetimepicker.zh-CN.js"></script>
    <!--提示框组件库 -->
    <script src="~/Scripts/toastr-master/toastr.min.js"></script>
    <!-- 弹窗组件 -->
    <script src="~/Scripts/bootbox.min.js"></script>
    <script src="~/Scripts/Highcharts-6.2.0/code/highcharts.js"></script>
    <script src="~/Scripts/Highcharts-6.2.0/code/highcharts-more.js"></script>
    <script src="~/Scripts/Highcharts-6.2.0/code/modules/solid-gauge.js"></script>
    <script src="~/Scripts/Highcharts-6.2.0/code/highcharts-zh_CN.js"></script>
</head>
<body>
    <div>
        <ol class="breadcrumb Shoev">
            <li><span style="color:#337ab7;">@ViewBag.PageTitle</span></li>
            <li class="active">@ViewBag.PageColumn</li>
        </ol>
        @RenderBody()
    </div>
</body>
</html>
